<h1>投稿一覧</h1>

<table>
  <tr>
    <th>content</th>
  </tr>

  <% @blogs.each do |blog| %>
    <tr>
      <td>
        <%= blog.content %>
      </td>
      <td>
        <%= link_to "詳細", blog_path(blog.id) %>
        <%= link_to "編集", edit_blog_path(blog.id) %>
        <%= link_to "削除", blog_path(blog.id), method: :delete %>
      </td>
    </tr>
  <% end %>
</table>

<%= link_to "新規作成", new_blog_path %>

